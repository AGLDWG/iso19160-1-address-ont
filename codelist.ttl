    dcterms:isFormatOf :AddressTypology ;
    skos:prefLabel "AddressTypology - ConceptScheme" .

<AddressAliasType> a skos:ConceptScheme ;
    dcterms:isFormatOf :AddressAliasType ;
    skos:prefLabel "AddressAliasType - ConceptScheme" .

