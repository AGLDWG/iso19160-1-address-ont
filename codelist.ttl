    dcterms:isFormatOf :AddressComponentValueType ;
    skos:prefLabel "AddressComponentValueType - ConceptScheme" .

<AddressLifecycleStage> a skos:ConceptScheme ;
    dcterms:isFormatOf :AddressLifecycleStage ;
    skos:prefLabel "AddressLifecycleStage - ConceptScheme" .

<AddressableObjectLifecycleStage> a skos:ConceptScheme ;
    dcterms:isFormatOf :AddressableObjectLifecycleStage ;
    skos:prefLabel "AddressableObjectLifecycleStage - ConceptScheme" .

<AddressComponentType> a skos:ConceptScheme ;
    dcterms:isFormatOf :AddressComponentType ;
    skos:prefLabel "AddressComponentType - ConceptScheme" .
